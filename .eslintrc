{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "Buffer": true,
    "define": true,
    "__DEV__": true,
    "__dirname": false,
    "__fbBatchedBridgeConfig": false,
    "alert": false,
    "cancelAnimationFrame": false,
    "cancelIdleCallback": false,
    "clearImmediate": true,
    "clearInterval": false,
    "clearTimeout": false,
    "console": false,
    "document": false,
    "escape": false,
    "Event": false,
    "EventTarget": false,
    "exports": false,
    "fetch": false,
    "FormData": false,
    "global": false,
    "jest": false,
    "Map": true,
    "module": false,
    "navigator": false,
    "process": false,
    "Promise": true,
    "requestAnimationFrame": true,
    "requestIdleCallback": true,
    "require": false,
    "Set": true,
    "setImmediate": true,
    "setInterval": false,
    "setTimeout": false,
    "window": false,
    "XMLHttpRequest": false,
    "pit": false
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["babel", "prettier", "jest", "react-native", "react"],
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-throw-literal": 0,
    "camelcase": 0,
    "eqeqeq": 0,
    "no-plusplus": 0,
    "global-require": 0,
    "import/no-named-as-default": 0,
    "class-methods-use-this": 0,
    "sort-keys": 1,
    "sort-vars": 1,
    "no-unused-vars": 0
  }
}
